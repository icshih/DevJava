<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gaia.cu1.mdb.cu4.du439.dm">

	<class name="NSSParam" table="NSSPARAM">
		<id name="pid" type="long" column="PID">
		</id>
		<property name="name" type="String" column="NAME" />
		<property name="isUsed" type="boolean" column="IS_USED" />
		<property name="isConstrained" type="boolean" column="IS_CONSTRAINED" />
		<property name="isDerived" type="boolean" column="IS_DERIVED" />
		<property name="value" type="double" column="VALUE" />
		<property name="error" type="double" column="ERROR" />
		<property name="indexCorrMat" type="short" column="INDEX_CORR_MAT" />
	</class>

</hibernate-mapping>
